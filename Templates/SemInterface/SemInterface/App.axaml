             xmlns:local="clr-namespace:SemInterface"
             x:Class="SemInterface.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

  <Application.Styles>
    <StyleInclude Source="avares://AvaloniaEdit/AvaloniaEdit.xaml" />
